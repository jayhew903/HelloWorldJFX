<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="633.0" prefWidth="589.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.AptSchedulerCxController">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Customer Data">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TableView fx:id="customerTV" layoutX="2.0" layoutY="89.0" prefHeight="421.0" prefWidth="587.0">
            <columns>
                <TableColumn fx:id="CustomerIdTC" text="Customer ID" />
                <TableColumn fx:id="customerNameTC" prefWidth="118.0" text="Customer Name" />
                <TableColumn fx:id="addressTC" prefWidth="118.0" text="Address" />
                <TableColumn fx:id="postalCodeTC" prefWidth="76.0" text="Postal Code" />
                <TableColumn fx:id="phoneTC" prefWidth="96.0" text="Phone Number" />
                <TableColumn fx:id="fldIdTC" prefWidth="96.0" text="Division ID" />
            </columns>
        </TableView>
        <Button fx:id="reportsBtn" layoutX="14.0" layoutY="535.0" mnemonicParsing="false" onAction="#openReports" text="Reports" />
        <Button fx:id="addCxBtn" layoutX="86.0" layoutY="536.0" mnemonicParsing="false" onAction="#openAddCx" text="Add Customer" />
        <Button fx:id="modifyCxBtn" layoutX="193.0" layoutY="536.0" mnemonicParsing="false" onAction="#openModifyCx" text="Modify Customer" />
        <Button fx:id="deleteCxBtn" layoutX="315.0" layoutY="536.0" mnemonicParsing="false" onAction="#DeleteCx" text="Delete Customer" />
        <Button fx:id="logoutBtn" layoutX="517.0" layoutY="591.0" mnemonicParsing="false" onAction="#openLogin" text="Logout" />
        <Button fx:id="backBtn" layoutX="450.0" layoutY="591.0" mnemonicParsing="false" onAction="#backToAptScheduler" text="Back" />
      <Label fx:id="theLabel" layoutX="165.0" layoutY="47.0" text="Label" />
    </children>
</AnchorPane>
