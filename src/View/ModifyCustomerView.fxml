<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="452.0" prefWidth="421.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ModifyCustomerController">
    <children>
        <Label layoutX="23.0" layoutY="14.0" text="Modify Customer">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <Label layoutX="35.0" layoutY="142.0" text="Address" />
        <Label layoutX="24.0" layoutY="72.0" text="Customer ID" />
        <Label layoutX="13.0" layoutY="106.0" text="Customer Name" />
        <Label layoutX="17.0" layoutY="205.0" text="Phone Number" />
        <Label layoutX="25.0" layoutY="174.0" text="Postal Code" />
        <Button fx:id="saveCxBtn" layoutX="51.0" layoutY="325.0" mnemonicParsing="false" onAction="#saveCx" text="Save" />
        <Button fx:id="cxCancelBtn" layoutX="156.0" layoutY="326.0" mnemonicParsing="false" onAction="#cancelCx" text="Cancel" />
        <TextField fx:id="cxIdTxt" disable="true" layoutX="114.0" layoutY="68.0" />
        <TextField fx:id="cxNameTxt" layoutX="114.0" layoutY="102.0" />
        <TextField fx:id="cxAddressTxt" layoutX="114.0" layoutY="138.0" />
        <TextField fx:id="cxPostalCodeTxt" layoutX="114.0" layoutY="170.0" />
        <TextField fx:id="cxPhoneTxt" layoutX="114.0" layoutY="201.0" />
        <ComboBox fx:id="cxCountryCB" layoutX="19.0" layoutY="260.0" onAction="#selectFLD" prefHeight="26.0" prefWidth="167.0" promptText="Select Country" visibleRowCount="50" />
        <ComboBox fx:id="cxStateCB" layoutX="230.0" layoutY="260.0" prefHeight="26.0" prefWidth="167.0" promptText="Select State/Province" visibleRowCount="50" />
    </children>
</AnchorPane>
